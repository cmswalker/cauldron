Login

<%= form_for @user, url: "/sessions", method: "post" do |f| %>

	<div class="field">
	  <%= f.label :username %><br>
	  <%= f.text_field :username %>
	</div>
	<div class="field">
	  <%= f.label :password %><br>
	  <%= f.text_field :password %>
	</div>
	<div class="actions">
	  <%= f.submit "Log In" %>
	</div>
<% end %> 

or <%= link_to "Register", new_user_path %>